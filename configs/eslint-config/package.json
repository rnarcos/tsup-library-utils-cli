{
  "private": false,
  "name": "@libsync/eslint-config",
  "description": "ESLint configuration for libsync projects",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Marcos Fernandes",
    "url": "https://github.com/rnarcos"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rnarcos/libsync.git",
    "directory": "configs/eslint-config"
  },
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "libsync build",
    "clean": "libsync clean",
    "dev": "libsync dev --",
    "dev:watch": "pnpm dev --watch",
    "format": "prettier --check \"src/**/*.{js,cjs,json,md}\"",
    "format:fix": "prettier --write \"src/**/*.{js,cjs,json,md}\"",
    "lint": "eslint \"src/**/*.{js,cjs}\"",
    "lint:fix": "eslint \"src/**/*.{js,cjs}\" --fix"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0 || ^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0"
  },
  "exports": {
    "./base": {
      "import": "./src/base.js",
      "require": "./src/base.js"
    },
    "./node": {
      "import": "./src/node.js",
      "require": "./src/node.js"
    },
    "./react": {
      "import": "./src/react.js",
      "require": "./src/react.js"
    },
    "./typescript": {
      "import": "./src/typescript.js",
      "require": "./src/typescript.js"
    },
    "./package.json": "./package.json"
  }
}
