{
  "private": true,
  "name": "@libsync/prettier-config",
  "description": "Prettier configuration for libsync projects",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Marcos Fernandes",
    "url": "https://github.com/rnarcos"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rnarcos/libsync.git",
    "directory": "configs/prettier-config"
  },
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "libsync build",
    "clean": "libsync clean",
    "dev": "libsync dev",
    "dev:watch": "libsync dev --watch",
    "format": "prettier --check \"src/**/*.{js,cjs,json,md}\"",
    "format:fix": "prettier --write \"src/**/*.{js,cjs,json,md}\"",
    "lint": "eslint \"src/**/*.{js,cjs}\"",
    "lint:fix": "eslint \"src/**/*.{js,cjs}\" --fix"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@types/prettier": "^3.0.0",
    "eslint": "^9.35.0",
    "prettier": "^3.2.5"
  },
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.js"
    },
    "./base": {
      "import": "./src/base.js",
      "require": "./src/base.js"
    },
    "./web": {
      "import": "./src/web.js",
      "require": "./src/web.js"
    },
    "./package.json": "./package.json"
  }
}
